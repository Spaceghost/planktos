{
  "name": "planktos",
  "version": "0.1.1",
  "description": "Easily serve static website assets over bittorrent",
  "main": "index.js",
  "bin": "./bin/setup.js",
  "dependencies": {
    "chunk-store-stream": "^2.0.2",
    "create-torrent": "^3.25.1",
    "debug": "^2.3.3",
    "idb-kv-store": "^2.2.2",
    "indexdb-chunk-store": "^1.0.3",
    "minimist": "^1.2.0",
    "parse-torrent-file": "^4.0.0",
    "run-parallel-limit": "^1.0.3",
    "stream-to-blob": "^1.0.0",
    "webtorrent": "^0.97.2"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "express": "^4.14.0",
    "standard": "^8.6.0"
  },
  "scripts": {
    "standard": "standard",
    "bundle": "browserify injection.js -o build/injection.js --debug && browserify sw.js -o build/sw.js --debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuset/planktos.git"
  },
  "keywords": [
    "bittorrent",
    "webtorrent",
    "p2p",
    "static",
    "seed"
  ],
  "standard": {
    "globals": [
      "URL",
      "Response",
      "BroadcastChannel",
      "Blob"
    ],
    "ignore": [
      "build"
    ]
  },
  "author": "xuset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuset/planktos/issues"
  },
  "homepage": "https://xuset.github.io/planktos/"
}
